

<form action="/Party/addData">
    <div class="form-group">
        <label for="exampleInputEmail1">Name</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter name" name="Name">
       
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Email</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email" name="Email">

    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">phone</label>
        <input type="number" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter phone" name="Phone">
        
    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">message</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter massage" name="Msg">

    </div>

    <label for="exampleInputEmail1">Will Attended</label>
    <div class="form-check">
        <input class="form-check-input" type="radio" value="false" id="flexCheckDefault" name="Attend">
        <label class="form-check-label" for="flexCheckDefault">
            not attend
        </label>
    </div>
    <div class="form-check">
        <input class="form-check-input" type="radio" value="true" id="flexCheckChecked" checked name="Attend">
        <label class="form-check-label" for="flexCheckChecked">
            attend
        </label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

